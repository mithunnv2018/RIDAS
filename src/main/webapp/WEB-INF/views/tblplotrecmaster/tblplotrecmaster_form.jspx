	    	    opt2.text = obj.plotNo;
	    	    opt2.value=obj.plotPk;
	    	    x.add(opt2);
	            
		    	}
			 	   
		},
		error : function(e) {
			alert('Error121212: ' + JSON.stringify(e));
		}
	});
}

